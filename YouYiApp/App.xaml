<Application x:Class="YouYiApp.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:YouYiApp" 
             xmlns:converter="clr-namespace:YouYiApp.common"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="pack://application:,,,/Panuon.UI.Silver;component/Control.xaml" />
                <ResourceDictionary Source="pack://application:,,,/YouYiApp;component/Resources/Styles.xaml" />
            </ResourceDictionary.MergedDictionaries>
            <converter:RadioButtonConverter x:Key="RadioButtonConverter" />
            <converter:TextBoxConverter x:Key="TextBoxConverter" />
        </ResourceDictionary>

    </Application.Resources>
</Application>
